    <validationRules>
        <fullName>Approve_w_Comments_Validation</fullName>
        <active>true</active>
        <description>Make sure the status is approved before the system changes the status to Approved w Comments</description>
        <errorConditionFormula>AND(TEXT( PRIORVALUE(Approval_Status__c)) &lt;&gt; &apos;Approved&apos;, TEXT(Approval_Status__c) = &apos;Approved w Comments&apos; )</errorConditionFormula>
        <errorMessage>&apos;&apos;You must approve this step before setting the status to &quot;Approve w Comments&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Edit_Approver_Upon_Approve_Reject</fullName>
        <active>true</active>
        <description>User should not be able to change the assigned approver once the approval status has changed to Approved, Approved w Comments, or Rejected.</description>
        <errorConditionFormula>AND(
  OR(
    ISPICKVAL(Approval_Status__c, &quot;Approved&quot;),
    ISPICKVAL(Approval_Status__c, &quot;Approved w Comments&quot;),
    ISPICKVAL(Approval_Status__c, &quot;Rejected&quot;)
  ),
  ISCHANGED(Assigned_Approver__c)
)</errorConditionFormula>
        <errorMessage>The assigned approver cannot be changed once the approval step has been approved or rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_must_be_later_than_Start_Date</fullName>
        <active>true</active>
        <description>Due date must be later than start date.</description>
        <errorConditionFormula>IF( $CustomMetadata.CDT_Setting__mdt.Enhancement_Release.Allow_Validation__c, 

IF(ISNEW(), Due_Date__c &lt; Start_Date__c, 
IF( 
AND(NOT(ISPICKVAL(Approval_Status__c,&apos;Initiated/Closed&apos;)), 
NOT(ISPICKVAL(Approval_Status__c,&apos;Initiated/Reopened&apos;))) , Due_Date__c &lt; Start_Date__c, false) 
),

False)</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>The Due Date must be later than the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Specify_Ext_Review_Party</fullName>
        <active>true</active>
        <errorConditionFormula>IF( $CustomMetadata.CDT_Setting__mdt.Enhancement_Release.Allow_Validation__c, 

AND (ISBLANK( External_Review_Party__c ),  ISPICKVAL(  Role_of_Approver__c  , &quot;External Review&quot;)),

False)</errorConditionFormula>
        <errorDisplayField>External_Review_Party__c</errorDisplayField>
        <errorMessage>If &quot;External Review&quot; is selected as the Role of Approver, you must input a value in the &quot;External Review Party&quot; field.</errorMessage>
    </validationRules>
