    <webLinks>
        <fullName>Proxy_Approval_by_Exec_Sec</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Proxy Approval by Exec Sec</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 

{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} 


var NOT_EXECSEC_ADMIN = -1; 

var NOT_INITIATED = 0; 

var ALLOWED = 1; 

var ALREADY_EXISTS = 2; 


var astepId = &quot;{!Approval_Step__c.Id}&quot;; 


var callback_method = { 

onSuccess: handleSuccess, 

onFailure: handleFailure 

}; 


function handleSuccess(result) { 

switch(parseInt(result)) { 

case NOT_EXECSEC_ADMIN: 

console.log(&apos;NOT_EXECSEC_ADMIN&apos;); 

alert(&apos;You are not an ExecSec Admin. This action is not available to you.&apos;); 

break; 

case NOT_INITIATED: 

console.log(&apos;NOT_INITIATED&apos;); 

alert(&apos;The step needs to be initiated in order for this button to work.&apos;); 

break; 

case ALLOWED: 

alert(&apos;Your request for Proxy Access is in process. You will receive an email once the request is complete.&apos;); 

console.log(&apos;ALLOWED&apos;); 

break; 

case ALREADY_EXISTS: 

alert(&apos;You already have a pending request to reassign this approval step. Please allow the system some time to process it.&apos;); 

console.log(&apos;ALREADY_EXISTS&apos;); 

break; 

default: 

alert(&apos;Unexpected error. Please contact an admin. (CDTBatchUtil.cls)&apos;); 

console.log(&apos;This was not anticipated. Did someone change the constants?&apos;); 

} 

console.log(result); 

} 


function handleFailure(error) { 

alert(&apos;There was an issue processing your request. Please contact a Salesforce admin. Error: &apos; + error); 

} 

sforce.apex.execute(&quot;CDTBatchUtil&quot;,&quot;reassignExecSecUsers&quot;,{astep:astepId}, callback_method);</url>
    </webLinks>
