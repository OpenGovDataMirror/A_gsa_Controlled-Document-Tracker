    <webLinks>
        <fullName>Approve_w_Comments</fullName>
        <availability>online</availability>
        <description>After controlled document is approved, clicking Approve w Comments changes the approval status and overall status to &quot;Approved w Comments&quot;</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Approve w Comments</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/connection.js&quot;)} 

try {
var url = parent.location.href; 
var updateRecords = []; 
//alert(&apos;You must approve this step before setting the status to &quot;Approve w Comments&quot;&apos;);
var update_App = new sforce.SObject(&quot;Approval_Step__c&quot;); 
update_App.Id =&quot;{!Approval_Step__c.Id}&quot;; 
update_App.Approval_Status__c=&quot;Approved w Comments&quot;;
updateRecords.push(update_App);

//alert(url);
result = sforce.connection.update(updateRecords); 
//alert(result);
if(result &amp;&amp; (result + &apos;&apos;).indexOf(&quot;success:&apos;true&apos;&quot;) != -1) {
alert(&apos;Status updated successfully!&apos;); parent.location.href = url;
}

else {
alert(&apos;You must approve this step before setting the status to &quot;Approve w Comments&quot;&apos;);
}

} catch (e) {
alert (e);
}</url>
    </webLinks>
